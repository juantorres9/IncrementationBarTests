<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/layout_5"
    tools:context="com.example.myapplication.Main5Activity">


    <TextView
        android:layout_margin="100dp"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:background="#445577"
        android:text="TOTOTOTO"
        android:textColor="#ff4411"/>
    <RelativeLayout
        android:layout_margin="20dp"
        android:layout_width="90dp"
        android:layout_height="65dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true">

        <include layout="@layout/composed_button2"
            android:id="@+id/button_waiting_start_right"/>
    </RelativeLayout>

    <RelativeLayout
        android:layout_margin="20dp"
        android:layout_width="125dp"
        android:layout_height="65dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true">

        <include layout="@layout/composed_button_validate"
            android:id="@+id/button_waiting_start_right"/>
    </RelativeLayout>


</RelativeLayout>
